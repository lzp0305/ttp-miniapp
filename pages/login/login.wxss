.cover {
	width: 100%;
	height: 100vh;
	background-color: #2865b8;
	position: relative;
}
.center_box {
	width: 600rpx;
	height: 800rpx;
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	transform: translate(-50%,-50%);
	background-color: #fff;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	padding: 30rpx;
}
.notice {
	width: 100%;
	height: 60rpx;
	font-size: 20rpx;
	line-height: 30rpx;
	text-align: left;
}
.list_item {
	height: 100rpx;
	border-bottom: 1rpx solid #efeff4;
	position: relative;
}
label {
	display: block;
	width: 120rpx;
	font-size: 30rpx;
	line-height: 100rpx;
	height: 100rpx;
	color: #3b3b3b;
	text-align: justify;
	float: left;
}
label:after {
	content:'';
	display: inline-block;
	width: 100%;
}
input {
	width: 415rpx;
	height: 100rpx;
	font-size: 24rpx;
	line-height: 100rpx;
	color: #999;
	text-align: left;
	float: right;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	overflow: hidden;
}
.get_security {
	position: absolute;
	right: 0;
	top: 25rpx;
	width: 180rpx;
	height: 50rpx;
	font-size: 24rpx;
	line-height: 50rpx;
	text-align: center;
	color: #fff;
	background-color: #dcdcdc;
	border: 0;
	outline: none;
	border-radius: 0;
	padding: 0;
	margin: 0;
	z-index: 10;
}
.can_click {
	background-color: #2865b8!important;
}
.dont_click {
	background-color: #dcdcdc!important;
}
.get_security:after {
	border: 0;
	color: #fff!important;
}

.submit_btn {
	width: 100%;
	height: 75rpx;
	margin: 100rpx auto 0;
	background-color: #3566b2;
	color: #fff;
	font-size: 28rpx;
	line-height: 75rpx;
	border-radius: 10rpx;
	text-align: center;
}