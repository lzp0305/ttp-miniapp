
.card {
	width: 100%;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	padding: 0 30rpx;
	border-bottom: 20rpx solid #eeeeee;
	overflow: hidden;
}



.stitle {
	width: 100%;
	height: 69rpx;
	font-size: 30rpx;
	line-height: 69rpx;
	color: #454545;
	border-bottom: 1rpx solid #dcdcdc;
}
.stitle text {
	color: #2865b8;
}
.record_box {
	padding-bottom: 40rpx;
}
.record {
	margin-top: 20rpx;
	border-top: 1rpx solid #dcdcdc;
	width: 100%;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
.ret {
	font-size: 24rpx!important;
	color: #000!important;
}
.reone {
	width: 15%;
	height: 59rpx;
	line-height: 59rpx;
	text-align: center;
	border-bottom: 1rpx solid #dcdcdc;
	float: left;
	position: relative;
	font-size: 24rpx;
	color: #2865b8;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}
