.state0 {
	width: 70rpx;
	height: 35rpx;
	background-color: #2865b8;
	font-size: 20rpx;
	line-height: 35rpx;
	border-radius: 2rpx;
	color: #fff;
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	transform: translate(-50%,-50%);
}
