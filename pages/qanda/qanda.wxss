.card {
	width: 100%;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	padding: 0 30rpx;
	border-bottom: 20rpx solid #eeeeee;
	overflow: hidden;
}
.stitle {
	width: 100%;
	height: 69rpx;
	font-size: 30rpx;
	line-height: 69rpx;
	color: #454545;
	border-bottom: 1rpx solid #dcdcdc;
}
.top_qa {
	margin-top: 20rpx;
	padding-bottom: 34rpx;
}
.item_q {
	width: 100%;
	padding-left: 50rpx;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	background-image: url(http://wechat.jxlll.wang/images/q.png);
	background-repeat: no-repeat;
	background-size: 36rpx 36rpx;
	background-position: 0 0;
	font-size: 24rpx;
	line-height: 36rpx;
	color: #333;
	font-weight: bold;
}

.item_a {
	width: 100%;
	padding-left: 50rpx;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	background-image: url(http://wechat.jxlll.wang/images/a.png);
	background-repeat: no-repeat;
	background-size: 36rpx 36rpx;
	background-position: 0 1rpx;
	font-size: 24rpx;
	line-height: 38rpx;
	margin-top: 20rpx;
	color: #767676;
}